terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4"
    }
  }
}

provider "aws" {
  profile = var.profile
  region  = var.default-region

  default_tags {
    tags = {
      CustodianRule = "ecc-k8s-061-minimize_the_admission_of_windows_hostprocess_containers"
    }
  }
}
